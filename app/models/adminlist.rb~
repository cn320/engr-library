class Adminlist < ActiveRecord::Base
  
  def Adminlist.authenticate username, password
    if user = Adminlist.find_by_username(username)
      if password == user.password 
        user
      end
    end
  end
  
  def Adminlist.createacc username, password
    @newUser = Adminlist.new(:username=>username,:password=>password)
    if @newUser.save
       true
    else 
       false
    end
  end

end
